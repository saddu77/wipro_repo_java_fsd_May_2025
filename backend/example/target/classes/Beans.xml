<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
	<bean id="userBean" class="com.example.User">
		<property name="username" value="Alex"></property>
		<property name="password" value="123456"></property>
	</bean>
	
	<bean id="productBean" class="com.example.Product">
		<property name="p_id" value="101"></property>
		<property name="p_name" value="Laptop"></property>
	</bean>
</beans>